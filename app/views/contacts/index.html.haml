- content_for :menu do
  = menu_item "New", new_contact_path

%table
  %tr
    %th Name
    %th Telephone number
  - @contacts.each do |contact|
    %tr
      %td= link_to contact.name, contact_path(contact)
      %td= link_to contact.number, telephone_number_path(contact.telephone_number)

= will_paginate @contacts
